                    </tbody>
                </table>
            </div>
        </div>
        <div class="col-md-3 offset-lg-1">
            <div class="row row-cols-2 row-cols-md-1">
                <div class="col">
                    <img src="~/assets/img/landing/product/specs/01.png" alt="Image">
                </div>
                <div class="col">
                    <img src="~/assets/img/landing/product/specs/02.png" alt="Image">
                </div>
                <div class="col">
                    <img src="~/assets/img/landing/product/specs/03.png" alt="Image">
                </div>
                <div class="col">
                    <img src="~/assets/img/landing/product/specs/04.png" alt="Image">
                </div>
            </div>
        </div>
    </div>
</section>


<!-- Colors -->
