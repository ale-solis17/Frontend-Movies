
@{
    ViewBag.Title = "product";
    ViewBag.ActiveItem = "landing";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section styles{

    <link rel="stylesheet" media="screen" href="~/assets/vendor/boxicons/css/boxicons.min.css">
    <link rel="stylesheet" media="screen" href="~/assets/vendor/swiper/swiper-bundle.min.css">
    <link rel="stylesheet" media="screen" href="~/assets/vendor/lightgallery/css/lightgallery-bundle.min.css">
}
<header class="header navbar navbar-expand-lg navbar-dark position-absolute navbar-sticky">
    <div class="container px-3">
        <a href="/" class="navbar-brand pe-3">
            <img src="~/assets/img/logo.svg" width="47" alt="Silicon">
            Silicon
        </a>
        <div id="navbarNav" class="offcanvas offcanvas-end bg-dark">
            <div class="offcanvas-header border-bottom border-light">
                <h5 class="offcanvas-title text-white">Menu</h5>
                <button type="button" class="btn-close btn-close-white" data-bs-dismiss="offcanvas" aria-label="Close"></button>
            </div>
            <div class="offcanvas-body">

                @Html.Partial("~/Views/Shared/header.cshtml")

            </div>
            <div class="offcanvas-header border-top border-light">
                <a href="#!" class="btn btn-primary w-100" target="_blank" rel="noopener">
                    <i class="bx bx-cart fs-4 lh-1 me-1"></i>
                    &nbsp;Buy now
                </a>
            </div>
        </div>
        <div class="pe-lg-1 ms-auto me-4" data-bs-theme="dark">
            <div class="form-check form-switch mode-switch" data-bs-toggle="mode">
                <input type="checkbox" class="form-check-input" id="theme-mode">
                <label class="form-check-label d-none d-sm-block" for="theme-mode">Light</label>
                <label class="form-check-label d-none d-sm-block" for="theme-mode">Dark</label>
            </div>
        </div>
        <button type="button" class="navbar-toggler" data-bs-toggle="offcanvas" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <a href="#!" class="btn btn-primary btn-sm fs-sm rounded d-none d-lg-inline-flex" target="_blank" rel="noopener">
            <i class="bx bx-cart fs-5 lh-1 me-1"></i>
            &nbsp;Buy now
        </a>
    </div>
</header>

<!-- Hero section -->
<section class="min-vh-100 bg-dark bg-size-cover bg-repeat-0 bg-position-center position-relative overflow-hidden py-5 mb-5" style="background-image: url('/assets/img/landing/product/hero/bg.jpg');" data-bs-theme="dark">
    <div class="container pt-lg-3">
        <div class="row flex-lg-nowrap">
            <div class="col-lg-7 col-xl-6 col-xxl-5 text-center text-lg-start pt-4 pt-md-5 mt-xxl-4">
                <p class="text-body lead pt-5 mt-sm-2 mt-lg-4 mt-xl-5">Make your moments fly with</p>
                <h1 class="display-3 pb-md-2 mb-5">ProDrone 2.0</h1>
                <div class="mt-n4 ms-lg-n4 ms-n3 pb-sm-3 pb-md-4">
                    <a href="https://www.youtube.com/watch?v=NJ3Z1hSOoMc" class="btn btn-lg btn-primary mt-lg-4 mt-3 ms-lg-4 ms-3" data-bs-toggle="video">
                        Watch video
                        <i class="bx bx-play ms-1 lead"></i>
                    </a>
                    <a href="#" class="btn btn-lg btn-light mt-lg-4 mt-3 ms-lg-4 ms-3">
                        Learn more
                        <i class="bx bx-right-arrow-alt ms-1 lead"></i>
                    </a>
                </div>
                <div class="swiper-slider mx-auto mt-5 pt-xl-5 pt-lg-2 pt-xl-4" style="max-width: 33rem;">
                    <div class="swiper" data-swiper-options='{
                  "slidesPerView": 1,
                  "effect": "coverflow",
                  "centeredSlides": true,
                  "loop": true,
                  "speed": 650,
                  "coverflowEffect": {
                    "rotate": 0,
                    "stretch": 0,
                    "depth": 560,
                    "modifier": 1,
                    "slideShadows": true
                  },
                  "autoplay": {
                    "delay": 6000,
                    "disableOnInteraction": false
                  },
                  "pagination": {
                    "el": ".swiper-pagination",
                    "type": "fraction"
                  },
                  "navigation": {
                    "prevEl": ".btn-prev",
                    "nextEl": ".btn-next"
                  }
                }'>
                        <div class="swiper-wrapper">
                            <div class="swiper-slide">
                                <img src="~/assets/img/landing/product/hero/slider/01.jpg" width="434" alt="Slider image" class="d-block mx-auto rounded-3">
                            </div>
                            <div class="swiper-slide">
                                <img src="~/assets/img/landing/product/hero/slider/02.jpg" width="434" alt="Slider image" class="d-block mx-auto rounded-3">
                            </div>
                            <div class="swiper-slide">
                                <img src="~/assets/img/landing/product/hero/slider/03.jpg" width="434" alt="Slider image" class="d-block mx-auto rounded-3">
                            </div>
                        </div>

                        <!-- Prev/Next buttons + count -->
                        <div class="d-flex justify-content-center align-items-center pb-3 pt-4 mt-3">
                            <button type="button" class="btn btn-prev btn-icon position-relative top-0 mt-0 ms-0 me-2" style="transform: none;" aria-label="Previous">
                                <i class="bx bx-chevron-left"></i>
                            </button>
                            <div class="swiper-pagination text-light opacity-70 fs-sm fw-medium position-relative top-0 w-auto" style="min-width: 70px;"></div>
                            <button type="button" class="btn btn-next btn-icon position-relative top-0 mt-0 ms-2 me-0" style="transform: none;" aria-label="Next">
                                <i class="bx bx-chevron-right"></i>
                            </button>
                        </div>
                    </div>
                </div>
            </div>

            <!-- Featured image -->
            <div class="d-none d-lg-block pt-5 mt-lg-4 mt-xl-5 ms-lg-n5 mx-xl-0">
                <img src="~/assets/img/landing/product/hero/image.png" class="d-block pt-xxl-4 mt-5" width="1058" alt="Hero image">
            </div>
        </div>
    </div>
</section>


<!-- Text + Video -->
<section class="mb-5 pt-md-2 pt-lg-4 pt-xl-5">
    <div class="container mt-4 pt-2">
        <div class="row row-cols-md-2 row-cols-1 gy-4 mb-5">
            <div class="col">
                <h2 class="h1 mb-0">Shoot like a pro with our new easy to use <span class="text-primary">ProDrone 2.0</span></h2>
            </div>
            <div class="col">
                <p class="mb-0 fs-lg">Featuring a 1-inch CMOS sensor that can shoot 4K/60fps videos and 20MP photos, ProDrone 2.0 grants filmmakers absolute creative freedom. It carefully optimizes user experience and intelligent features. </p>
            </div>
        </div>
        <div class="position-relative rounded-3 overflow-hidden">
            <div class="position-absolute top-0 start-0 w-100 h-100 d-flex align-items-center justify-content-center zindex-5">
                <a href="https://www.youtube.com/watch?v=aOwTUgqUfH8" class="btn btn-video btn-icon btn-xl stretched-link bg-white" data-bs-toggle="video" aria-label="Play video">
                    <i class="bx bx-play"></i>
                </a>
            </div>
            <span class="position-absolute top-0 start-0 w-100 h-100 bg-dark opacity-40"></span>
            <img src="~/assets/img/landing/product/video-cover.jpg" alt="Cover image">
        </div>
    </div>
</section>


<!-- Benefits -->
<section class="container mb-5 pt-md-2 pt-lg-4 pt-xl-5">
    <h2 class="h1 my-4 pt-2 text-center">The benefits of world-changing drone</h2>
