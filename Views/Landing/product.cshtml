                    <!-- Item -->
                    <div class="col">
                        <div class="card card-hover border-0 shadow position-relative">
                            <img src="~/assets/img/landing/product/colors/02.png" alt="Skin image" class="card-img-top">
                            <div class="card-body mt-n5 mx-3 mb-3 pt-0 text-center">
                                <span>White ProDrone 2.0</span>
                                <h3 class="mt-2 mb-4">$1100</h3>
                                <a href="#" class="btn btn-primary stretched-link">Buy now</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>


<!-- CTA -->
